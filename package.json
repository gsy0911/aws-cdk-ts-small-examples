{
  "name": "aws-cdk-ts-small-examples",
  "version": "0.1.0",
  "description": "contains a set of TypeScript example projects for the AWS Cloud Development Kit.",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node src/index.ts",
    "dev:watch": "ts-node-dev --respawn src/index.ts",
    "clean": "rimraf dist/*",
    "tsc": "tsc",
    "build": "npm-run-all clean lint:fix tsc",
    "start": "node .",
    "check-types": "tsc --noEmit",
    "eslint": "eslint src/**/*.ts",
    "eslint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write 'src/**/*.{js,ts,json}'",
    "lint": "npm-run-all eslint check-types",
    "lint:fix": "npm-run-all eslint:fix check-types format"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gsy0911/aws-cdk-ts-small-examples.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gsy0911/aws-cdk-ts-small-examples/issues"
  },
  "homepage": "https://github.com/gsy0911/aws-cdk-ts-small-examples#readme",
  "devDependencies": {
    "@aws-cdk/aws-apigateway": "^1.91.0",
    "@aws-cdk/aws-codebuild": "^1.91.0",
    "@aws-cdk/aws-codepipeline": "^1.91.0",
    "@aws-cdk/aws-codepipeline-actions": "^1.91.0",
    "@aws-cdk/aws-iam": "^1.91.0",
    "@aws-cdk/aws-lambda": "^1.91.0",
    "@aws-cdk/aws-lambda-python": "^1.91.0",
    "@aws-cdk/core": "^1.91.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^7.2.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rimraf": "^2.7.1",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.2"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "*.ts": "tsc --noEmit",
    "*.{js,ts,json}": "prettier --write"
  },
  "dependencies": {
    "@aws-cdk/aws-batch": "^1.91.0",
    "@aws-cdk/aws-ec2": "^1.91.0",
    "@aws-cdk/aws-ecr": "^1.91.0",
    "@aws-cdk/aws-ecs": "^1.91.0",
    "@aws-cdk/aws-stepfunctions": "^1.91.0",
    "@aws-cdk/aws-stepfunctions-tasks": "^1.91.0"
  }
}
