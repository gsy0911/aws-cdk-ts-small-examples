{
	"name": "aws-cdk-ts-small-examples",
	"version": "0.1.0",
	"description": "contains a set of TypeScript example projects for the AWS Cloud Development Kit.",
	"bin": {
		"example": "bin/index.js"
	},
	"scripts": {
		"build": "tsc",
		"watch": "tsc -w",
		"test": "jest",
		"cdk": "cdk"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/gsy0911/aws-cdk-ts-small-examples.git"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/gsy0911/aws-cdk-ts-small-examples/issues"
	},
	"homepage": "https://github.com/gsy0911/aws-cdk-ts-small-examples#readme",
	"dependencies": {
		"aws-cdk-lib": "2.2.0",
		"constructs": "^10.0.0",
		"source-map-support": "^0.5.16"
	},
	"devDependencies": {
		"@types/jest": "^26.0.10",
		"@types/node": "10.17.27",
		"jest": "^26.4.2",
		"ts-jest": "^26.2.0",
		"aws-cdk": "2.2.0",
		"ts-node": "^9.0.0",
		"typescript": "^4.4.2"
	},
	"lint-staged": {
		"*.{js,ts}": "eslint --cache --fix",
		"*.ts": "tsc --noEmit",
		"*.{js,ts,json}": "prettier --write"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"ts"
		],
		"transform": {
			"^.+\\.ts$": "ts-jest"
		},
		"globals": {
			"ts-jest": {
				"tsConfing": "tsconfig.json"
			}
		},
		"testMatch": [
			"**/*.test.ts"
		],
		"roots": [
			"<rootDir>/test"
		]
	}
}
