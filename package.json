{
	"name": "aws-cdk-ts-small-examples",
	"version": "0.1.0",
	"description": "contains a set of TypeScript example projects for the AWS Cloud Development Kit.",
	"main": "bin/index.js",
	"scripts": {
		"dev": "ts-node src/index.ts",
		"dev:watch": "ts-node-dev --respawn src/index.ts",
		"clean": "rimraf dist/*",
		"tsc": "tsc",
		"build": "tsc",
		"start": "node .",
		"check-types": "tsc --noEmit",
		"eslint": "eslint src/**/*.ts",
		"eslint:fix": "eslint src/**/*.ts --fix",
		"format": "prettier --write 'src/**/*.{js,ts,json}'",
		"lint": "npm-run-all eslint check-types",
		"lint:fix": "npm-run-all eslint:fix check-types format",
		"test": "jest"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/gsy0911/aws-cdk-ts-small-examples.git"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/gsy0911/aws-cdk-ts-small-examples/issues"
	},
	"homepage": "https://github.com/gsy0911/aws-cdk-ts-small-examples#readme",
	"devDependencies": {
		"@aws-cdk/assert": "^1.114.0",
		"@aws-cdk/aws-apigateway": "^1.121.0",
		"@aws-cdk/aws-batch": "^1.121.0",
		"@aws-cdk/aws-chatbot": "^1.121.0",
		"@aws-cdk/aws-cloudfront": "^1.116.0",
		"@aws-cdk/aws-cloudfront-origins": "^1.116.0",
		"@aws-cdk/aws-cloudtrail": "^1.121.0",
		"@aws-cdk/aws-codebuild": "^1.121.0",
		"@aws-cdk/aws-codedeploy": "^1.121.0",
		"@aws-cdk/aws-codepipeline": "^1.121.0",
		"@aws-cdk/aws-codepipeline-actions": "^1.121.0",
		"@aws-cdk/aws-cognito": "^1.121.0",
		"@aws-cdk/aws-config": "^1.121.0",
		"@aws-cdk/aws-ecr-assets": "^1.121.0",
		"@aws-cdk/aws-ecs-patterns": "^1.121.0",
		"@aws-cdk/aws-elasticloadbalancingv2": "^1.121.0",
		"@aws-cdk/aws-elasticloadbalancingv2-actions": "^1.116.0",
		"@aws-cdk/aws-events-targets": "^1.121.0",
		"@aws-cdk/aws-guardduty": "^1.121.0",
		"@aws-cdk/aws-iam": "^1.121.0",
		"@aws-cdk/aws-kinesisfirehose": "^1.121.0",
		"@aws-cdk/aws-lambda": "^1.121.0",
		"@aws-cdk/aws-lambda-python": "^1.121.0",
		"@aws-cdk/aws-logs": "^1.121.0",
		"@aws-cdk/aws-rds": "^1.121.0",
		"@aws-cdk/aws-route53": "^1.121.0",
		"@aws-cdk/aws-s3": "^1.121.0",
		"@aws-cdk/aws-sns": "^1.121.0",
		"@aws-cdk/aws-wafv2": "^1.121.0",
		"@aws-cdk/core": "^1.121.0",
		"@commitlint/cli": "^11.0.0",
		"@commitlint/config-conventional": "^11.0.0",
		"@types/jest": "^26.0.24",
		"@types/node": "^14.17.14",
		"@typescript-eslint/eslint-plugin": "^4.31.0",
		"@typescript-eslint/parser": "^4.31.0",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^7.2.0",
		"husky": "^4.3.8",
		"jest": "^26.6.3",
		"lint-staged": "^10.5.3",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.3.2",
		"rimraf": "^2.7.1",
		"ts-jest": "^26.5.0",
		"ts-node": "^9.1.1",
		"ts-node-dev": "^1.1.8",
		"typescript": "^4.4.2"
	},
	"lint-staged": {
		"*.{js,ts}": "eslint --cache --fix",
		"*.ts": "tsc --noEmit",
		"*.{js,ts,json}": "prettier --write"
	},
	"dependencies": {
		"@aws-cdk/aws-ec2": "^1.121.0",
		"@aws-cdk/aws-ecr": "^1.121.0",
		"@aws-cdk/aws-ecs": "^1.121.0",
		"@aws-cdk/aws-stepfunctions": "^1.121.0",
		"@aws-cdk/aws-stepfunctions-tasks": "^1.121.0"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"ts"
		],
		"transform": {
			"^.+\\.ts$": "ts-jest"
		},
		"globals": {
			"ts-jest": {
				"tsConfing": "tsconfig.json"
			}
		},
		"testMatch": [
			"**/*.test.ts"
		],
		"roots": [
			"<rootDir>/test"
		]
	}
}
